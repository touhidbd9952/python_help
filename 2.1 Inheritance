////Person Class////////////////////////////////////////////////////
class Person:
  def __init__(self, fname, lname):
    self.firstname = fname
    self.lastname = lname

  def printname(self):
    print(self.firstname, self.lastname)
 
 ////create "Student" class from "Person" class//////////////////
  class Student(Person):
    pass 
 
 
 /////Create Object/////////////////////////////////////////////
    x = Student("Mike", "Olsen")
    x.printname()
